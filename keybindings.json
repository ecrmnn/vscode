// Place your key bindings in this file to overwrite the defaultsauto[]
[
    {
        "key": "alt+enter",
        "command": "namespaceResolver.import",
        "when": "editorTextFocus",
    },
    {
        "key": "cmd+d",
        "command": "extension.caseSensitiveAddNext",
        "when": "editorTextFocus",
    },
    {
        "key": "cmd+r",
        "command": "workbench.action.tasks.runTask",
        "args": "phpunit-current-test",
        "when": "editorTextFocus && !editorReadonly && editorLangId == php"
    },
    {
        "key": "shift+cmd+r",
        "command": "workbench.action.tasks.runTask",
        "args": "phpunit-current-class",
        "when": "editorTextFocus && !editorReadonly && editorLangId == php"
    },
    {
        "key": "alt+cmd+l",
        "command": "editor.action.formatDocument",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+cmd+l",
        "command": "workbench.action.tasks.runTask",
        "args": "php-cs-fixer",
        "when": "editorTextFocus && !editorReadonly && editorLangId == php"
    },
    {
        "key": "alt+cmd+l",
        "command": "eslint.executeAutofix",
        "when": "editorTextFocus && !editorReadonly && editorLangId == javascript"
    },
    {
        "key": "alt+cmd+l",
        "command": "eslint.executeAutofix",
        "when": "editorTextFocus && !editorReadonly && editorLangId == vue"
    },
    
    {
        "key": "alt+up",
        "command": "editor.action.smartSelect.grow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+cmd+right",
        "command": "-editor.action.smartSelect.grow",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+down",
        "command": "editor.action.smartSelect.shrink",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+cmd+left",
        "command": "-editor.action.smartSelect.shrink",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+e",
        "command": "workbench.action.openNextRecentlyUsedEditorInGroup"
    },
    {
        "key": "cmd+numpad_divide",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+7",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+l",
        "command": "workbench.action.gotoLine"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine"
    },
    {
        "key": "ctrl+shift+space",
        "command": "editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+space",
        "command": "-editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "cmd+[Semicolon]",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+shift+[Equal]",
        "command": "-workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "cmd+g",
        "command": "editor.action.referenceSearch.trigger",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
        "key": "shift+f12",
        "command": "-editor.action.referenceSearch.trigger",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
        "key": "cmd+g",
        "command": "references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "shift+alt+f12",
        "command": "-references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "alt+cmd+left",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key": "alt+cmd+right",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward"
    }
]
